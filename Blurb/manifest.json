{
  "manifest_version": 2,

  "name": "Blurb",
  "description": "This extension takes user input and gives recommended news article for the user.",
  "version": "0.1",
  "icons": {  "16": "blurbicon.png",
              "48": "icon_48.png",
              "128":"icon_128.png" },

  "permissions": [
    "https://secure.flickr.com/"
  ],
  "browser_action": {
    "default_icon": "blurbicon.png",
    "default_popup": "popup.html"
  }
}