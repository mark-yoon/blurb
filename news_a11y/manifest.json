{
  "name": "News Reader",
  "version": "1.1",
  "description": "Displays the first 5 items from an RSS feed in a popup.",
  "icons": { "128": "news_icon.png" },
  "browser_action": {
    "default_title": "News",
    "default_icon": "news_action.png",
    "default_popup": "feed.html"
  },
  "permissions": [
    "tabs",
    "http://rss.cnn.com/*"
  ],
  "manifest_version": 2
}
